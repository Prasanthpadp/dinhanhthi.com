{
  "name": "eleventy-high-performance-blog",
  "version": "5.0.2",
  "license": "MIT",
  "description": "A starter repository for a blog web site using the Eleventy static site generator.",
  "scripts": {
    "build": "npm run js-build && eleventy --config=.eleventy.main.js",
    "build-old": "npm run js-build && eleventy --config=.eleventy.dev.js && npm run test",
    "watch": "concurrently \"eleventy --config=.eleventy.dev.js --serve\" \"npm run js-build-watch\" \"npm run test-watch\"",
    "thi": "concurrently \"eleventy --config=.eleventy.dev.js --serve\" \"npm run js-build-watch\"",
    "serve": "eleventy --config=.eleventy.dev.js --serve",
    "js-build": "rollup -c rollup.config.js",
    "js-build-watch": "rollup -c rollup.config.js -w",
    "debug": "DEBUG=* eleventy --config=.eleventy.dev.js && npm run test",
    "test": "mocha test/test*.js",
    "test-watch": "mocha test/test*.js --watch"
  },
  "pre-push": ["build"],
  "pre-commit": [],
  "repository": {
    "type": "git",
    "url": "git://github.com/11ty/eleventy-base-blog.git"
  },
  "bugs": {
    "url": "https://github.com/11ty/eleventy-base-blog/issues"
  },
  "homepage": "https://github.com/11ty/eleventy-base-blog#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-navigation": "^0.1.3",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "@gerhobbelt/markdown-it-inline-text-color": "^1.0.1-1",
    "@iktakahiro/markdown-it-katex": "^4.0.1",
    "eleventy-plugin-local-images": "^0.4.0",
    "eleventy-plugin-toc": "^1.1.0",
    "file-type": "^12.0.1",
    "fs-extra": "^8.1.0",
    "jsdom": "^15.1.1",
    "luxon": "^1.21.3",
    "markdown-it-anchor": "^5.2.5",
    "markdown-it-container": "^3.0.0",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-kbd": "^2.2.0",
    "markdown-it-task-lists": "^2.1.1",
    "node-fetch": "^2.6.1",
    "pre-commit": "^1.2.2",
    "pre-push": "^0.1.1",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-terser": "^6.1.0",
    "shorthash": "0.0.2"
  },
  "dependencies": {
    "@ampproject/toolbox-optimizer": "^1.1.2",
    "@saschazar/wasm-avif": "^1.0.0",
    "clean-css": "^4.2.3",
    "concurrently": "^5.2.0",
    "csp-hash-generator": "0.0.3",
    "csso": "^4.0.3",
    "datauri": "^3.0.0",
    "expect.js": "^0.3.1",
    "hasha": "^5.2.0",
    "html-minifier": "^4.0.0",
    "image-size": "^0.8.3",
    "lru-cache": "^5.1.1",
    "markdown-it": "^11.0.1",
    "markdown-it-attrs": "^3.0.3",
    "markdown-it-emoji": "^2.0.0",
    "markdown-it-mark": "^3.0.0",
    "markdown-it-table-of-contents": "^0.5.0",
    "mocha": "^8.1.3",
    "phin": "^3.5.0",
    "purge-from-html": "^1.0.3",
    "purgecss": "^2.3.0",
    "querystring": "^0.2.0",
    "rollup": "^2.23.0",
    "sharp": "^0.25.4",
    "twitter": "^1.7.1"
  }
}
